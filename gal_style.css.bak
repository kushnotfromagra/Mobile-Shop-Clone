body
{
	padding:0px;
	margin:0px;
}
header
{
	width:100%;
	height:60px;
	background-color:white;
	box-sizing:border-box;
	float:left;
	padding:10px;
	
}
#brandname
{
	padding:0px;
	margin:0px;
	font-family:times new roman;
	float:left;
	font-size:30px;
	color:black;
}
#tagline
{ 
padding:0px;
margin:0px;
float:right;
font-family:Cambria;
font-size:18px;
margin-top:10px;
color:black;
}

nav
{
	
    width:100%;
	height:130px;
	background-color:red;
	padding:10px;
	box-sizing:border-box;
}
#Search_box
{
	float:left;
	height:35px;
	margin-top:20px;
}
#search
{
	float:left;
	height:40px;
	margin-top:20px;
}

#menu
{ 
	margin:10px;
	width:600px;
	height:40px;

	float:right;
	margin-top:20px;
	
}
td
{
	width:20%;
	height:40px;
	color:black;
	text-align:center;
	background-color:white;
}
section
{
	
	width:100%;
	height:1300px;
	float:left;
	box-sizing:border-box;
	padding:28px;
}
#moreproduct-head
{
	width:100%;
	height:45px;
	float:left;
	margin-top:40px;
	box-sizing:border-box;
}

#moreprohead
{
	padding:0;
	margin:0;
	text-align:center;
	padding:8px;
	font-family:sans-serif;
}

#pro-img
{
	width:300px;
	height:350px;
	border-style:solid;
	border-color:#ccc;
	border-width:1px;
	float:left;
	box-sizing:border-box;
	margin:20px;
	margin-left:40px;
}

#order-now
{
	width:150px;
	height:50px;
	margin-left:70px;
	margin-top:15px;
	background-color:white;
	color:#323232;
	font-family:sans-serif;
	font-size:15px;
	border-color:#ccc;
}
footer
{
	width:100%;
	height:350px;
	float:left;
	background-color:red;
	box-sizing:border-box;
	padding:25px;
}
#contact
{
	width:350px;
	height:300px;
	float:left;
	box-sizing:border-box;
	margin-left:40px;
	line-height:28px;
}
#contact-us
{
	padding:0;
	margin:0;
	font-family:sans-serif;
	color:white;
	text-align:center;
}
h5
{
	padding:0;
	margin:0;
	font-family:sans-serif;
	font-size:18px;
	color:white;
}
address
{
	font-size:13px;
	color:white;
}

#map
{
	width:500px;
	height:300px;
	float:right;
	box-sizing:border-box;
}
